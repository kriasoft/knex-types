    "schema",
    "template",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "Kriasoft",
    "email": "hello@kriasoft.com",
    "url": "https://kriasoft.com"
  },
  "repository": "kriasoft/knex-types",
  "contributors": [
    "Konstantin Tarkus <hello@tarkus.me> (https://tarkus.me/)"
  ],
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/koistya"
  },
  "main": "main.js",
  "types": "main.d.ts",
  "scripts": {
